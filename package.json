{
  "name": "cpu-embeddings",
  "version": "1.1.0",
  "description": "embeddings generated on CPU for lightweight classification tasks",
  "main": "dist/src/index.js",
  "types": "dist/src/index.d.ts",
  "scripts": {
    "postinstall": "patch-package",
    "dev": "webpack serve --mode development",
    "build": "webpack --mode production",
    "test": "vitest",
    "format": "biome format --write src",
    "format:check": "biome format src",
    "release": "standard-version",
    "release:minor": "standard-version --release-as minor",
    "release:major": "standard-version --release-as major",
    "release:patch": "standard-version --release-as patch"
  },
  "keywords": [
    "embeddings",
    "cpu",
    "classification",
    "machine-learning"
  ],
  "author": "Sepehr Laal",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/3p3r/cpu-embeddings.git"
  },
  "bugs": {
    "url": "https://github.com/3p3r/cpu-embeddings/issues"
  },
  "homepage": "https://github.com/3p3r/cpu-embeddings#readme",
  "devDependencies": {
    "@biomejs/biome": "^2.2.4",
    "@types/node": "^24.5.2",
    "@types/webpack": "^5.28.5",
    "@types/webpack-dev-server": "^4.7.1",
    "@xenova/transformers": "^2.17.2",
    "standard-version": "^9.5.0",
    "ts-loader": "^9.5.4",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.2",
    "vitest": "^3.2.4",
    "webpack": "^5.101.3",
    "webpack-cli": "^6.0.1",
    "webpack-dev-server": "^5.2.2"
  },
  "dependencies": {
    "patch-package": "^8.0.0"
  },
  "files": [
    "dist",
    "models",
    "README.md",
    "LICENSE"
  ]
}
